AA,UADkB;AAExBf,gBAAAA,KAAK,CAACG,QAAN,GAAiBY,UAAU,CAACC,IAA5B;;AAFwB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGzB,KALQ;;AAMT;AACMC,IAAAA,cAPG,0BAOYjB,KAPZ,EAOmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACDJ,KAAK,CAACkB,GAAN,CAAU,kCAAV,CADC;;AAAA;AACpBI,gBAAAA,UADoB;AAE1BlB,gBAAAA,KAAK,CAACE,UAAN,GAAmBgB,UAAU,CAACF,IAA9B;;AAF0B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAG3B,KAVQ;AAWT;AACAG,IAAAA,eAZS,2BAYOnB,KAZP,EAYcoB,OAZd,EAYuB;AAC9BpB,MAAAA,KAAK,CAACC,SAAN,CAAgBoB,IAAhB,CAAqBD,OAArB,EAD8B,CAE9B;;AACAE,MAAAA,YAAY,CAACC,OAAb,CAAqB,kBAArB,EAAyCC,IAAI,CAACC,SAAL,CAAezB,KAAK,CAACC,SAArB,CAAzC,EAH8B,CAI9B;;AACAJ,MAAAA,QAAQ,CAAC6B,KAAT,CAAe,oBAAf;AACD,KAlBQ;AAmBTC,IAAAA,+BAnBS,2CAmBuB3B,KAnBvB,EAmB8B;AACrC;AACA,UAAG;AACD,YAAI4B,gBAAgB,GAAGJ,IAAI,CAACK,KAAL,CAAWP,YAAY,CAACQ,OAAb,CAAqB,kBAArB,CAAX,CAAvB;;AADC,mDAEeF,gBAFf;AAAA;;AAAA;AAED,8DAAkC;AAAA,gBAAvBG,CAAuB;AAChC/B,YAAAA,KAAK,CAACC,SAAN,CAAgBoB,IAAhB,CAAqBU,CAArB;AACD,WAJA,CAKD;;AALC;AAAA;AAAA;AAAA;AAAA;;AAMDlC,QAAAA,QAAQ,CAAC6B,KAAT,CAAe,oBAAf;AACD,OAPD,CAQA,gBAAK;AACHM,QAAAA,OAAO,CAACC,GAAR,CAAY,sBAAZ;AACD;AACF;AAhCQ;AAzBiB,CAAf,CAAf","sourcesContent":["import Vue from 'vue';\nimport Vuex from 'vuex';\nimport axios from 'axios';\nimport eventBus from '../eventbus';\n\nVue.use(Vuex);\n\nexport default new Vuex.Store({\n  state: {\n    warenkorb: [],\n    Kategorien: [],\n    produkte: [],\n  },\n\n  actions: {\n    LoadProducts(context) {\n      //Leitet zur mutation weiter\n      context.commit('loadProducts');\n    },\n    LoadCategories(context) {\n      //Leitet zur mutation weiter\n      context.commit('loadCategories');\n    },\n    PushInWarenkorb(context, item) {\n      //Um in den warenkorb zu pushen\n      context.commit('pushInWarenkorb', item);\n    },\n    ReloadWarenkorbFromLocalStorage(context) {\n      context.commit('reloadWarenkorbFromLocalStorage');\n    },\n  },\n\n  mutations: {\n    /*Ladet daten aus dem JSON in Produkte-Liste */\n    async loadProducts(state) {\n      const produclist = await axios.get('http://localhost:3000/productlist');\n      state.produkte = produclist.data;\n    },\n    /*Ladet daten aus dem JSON in Produkte-Liste */\n    async loadCategories(state) {\n      const categories = await axios.get('http://localhost:3000/kategorien');\n      state.Kategorien = categories.data;\n    },\n    //Pusht item in den Warenkorb\n    pushInWarenkorb(state, payload) {\n      state.warenkorb.push(payload);\n      //Warenkorb auch im LocalStorage speichern\n      localStorage.setItem('WarenkorbStorage', JSON.stringify(state.warenkorb));\n      //Batch aktuallisieren\n      eventBus.$emit('UpdateLocalStorage');\n    },\n    reloadWarenkorbFromLocalStorage(state) {\n      //Warenkorb mit LocalstorageDaten f√ºllen\n      try{\n        let StorageWarenkorb = JSON.parse(localStorage.getItem('WarenkorbStorage'));\n        for (const i of StorageWarenkorb) {\n          state.warenkorb.push(i);\n        }\n        //Batch updaten\n        eventBus.$emit('UpdateLocalStorage');\n      }\n      catch{\n        console.log(\"Warenkorb noch leer!\")\n      }\n    },\n  },\n});\n"]}]}