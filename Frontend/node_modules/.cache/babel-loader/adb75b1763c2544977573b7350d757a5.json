,GAAN,CAAU,mCAAV,CADD;;AAAA;AAClBC,gBAAAA,UADkB;AAExBf,gBAAAA,KAAK,CAACG,QAAN,GAAiBY,UAAU,CAACC,IAA5B;;AAFwB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGzB,KALQ;;AAMT;AACMC,IAAAA,cAPG,0BAOYjB,KAPZ,EAOmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACDJ,KAAK,CAACkB,GAAN,CAAU,qCAAV,CADC;;AAAA;AACpBI,gBAAAA,UADoB;AAE1BlB,gBAAAA,KAAK,CAACE,UAAN,GAAmBgB,UAAU,CAACF,IAA9B;;AAF0B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAG3B,KAVQ;AAWT;AACAG,IAAAA,eAZS,2BAYOnB,KAZP,EAYcoB,OAZd,EAYuB;AAC9BpB,MAAAA,KAAK,CAACC,SAAN,CAAgBoB,IAAhB,CAAqBD,OAArB,EAD8B,CAE9B;;AACAE,MAAAA,YAAY,CAACC,OAAb,CAAqB,kBAArB,EAAyCC,IAAI,CAACC,SAAL,CAAezB,KAAK,CAACC,SAArB,CAAzC,EAH8B,CAI9B;;AACAJ,MAAAA,QAAQ,CAAC6B,KAAT,CAAe,oBAAf;AACD,KAlBQ;AAmBTC,IAAAA,+BAnBS,2CAmBuB3B,KAnBvB,EAmB8B;AACrC;AACA,UAAG;AACD,YAAI4B,gBAAgB,GAAGJ,IAAI,CAACK,KAAL,CAAWP,YAAY,CAACQ,OAAb,CAAqB,kBAArB,CAAX,CAAvB;;AADC,mDAEeF,gBAFf;AAAA;;AAAA;AAED,8DAAkC;AAAA,gBAAvBG,CAAuB;AAChC/B,YAAAA,KAAK,CAACC,SAAN,CAAgBoB,IAAhB,CAAqBU,CAArB;AACD,WAJA,CAKD;;AALC;AAAA;AAAA;AAAA;AAAA;;AAMDlC,QAAAA,QAAQ,CAAC6B,KAAT,CAAe,oBAAf;AACD,OAPD,CAQA,gBAAK;AACHM,QAAAA,OAAO,CAACC,GAAR,CAAY,sBAAZ;AACD;AACF;AAhCQ;AAzBiB,CAAf,CAAf","sourcesContent":["import Vue from 'vue';\r\nimport Vuex from 'vuex';\r\nimport axios from 'axios';\r\nimport eventBus from '../eventbus';\r\n\r\nVue.use(Vuex);\r\n\r\nexport default new Vuex.Store({\r\n  state: {\r\n    warenkorb: [],\r\n    Kategorien: [],\r\n    produkte: [],\r\n  },\r\n\r\n  actions: {\r\n    LoadProducts(context) {\r\n      //Leitet zur mutation weiter\r\n      context.commit('loadProducts');\r\n    },\r\n    LoadCategories(context) {\r\n      //Leitet zur mutation weiter\r\n      context.commit('loadCategories');\r\n    },\r\n    PushInWarenkorb(context, item) {\r\n      //Um in den warenkorb zu pushen\r\n      context.commit('pushInWarenkorb', item);\r\n    },\r\n    ReloadWarenkorbFromLocalStorage(context) {\r\n      context.commit('reloadWarenkorbFromLocalStorage');\r\n    },\r\n  },\r\n\r\n  mutations: {\r\n    /*Ladet daten aus dem JSON in Produkte-Liste */\r\n    async loadProducts(state) {\r\n      const produclist = await axios.get('http://localhost:2410/getProducts');\r\n      state.produkte = produclist.data;\r\n    },\r\n    /*Ladet daten aus dem JSON in Produkte-Liste */\r\n    async loadCategories(state) {\r\n      const categories = await axios.get('http://localhost:2410/getCategories');\r\n      state.Kategorien = categories.data;\r\n    },\r\n    //Pusht item in den Warenkorb\r\n    pushInWarenkorb(state, payload) {\r\n      state.warenkorb.push(payload);\r\n      //Warenkorb auch im LocalStorage speichern\r\n      localStorage.setItem('WarenkorbStorage', JSON.stringify(state.warenkorb));\r\n      //Batch aktuallisieren\r\n      eventBus.$emit('UpdateLocalStorage');\r\n    },\r\n    reloadWarenkorbFromLocalStorage(state) {\r\n      //Warenkorb mit LocalstorageDaten f√ºllen\r\n      try{\r\n        let StorageWarenkorb = JSON.parse(localStorage.getItem('WarenkorbStorage'));\r\n        for (const i of StorageWarenkorb) {\r\n          state.warenkorb.push(i);\r\n        }\r\n        //Batch updaten\r\n        eventBus.$emit('UpdateLocalStorage');\r\n      }\r\n      catch{\r\n        console.log(\"Warenkorb noch leer!\")\r\n      }\r\n    },\r\n  },\r\n});\r\n"]}]}