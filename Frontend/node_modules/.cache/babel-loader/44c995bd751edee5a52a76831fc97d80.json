AAA,KAAA,gBAJA;AAKA,UAAA,SAAA,EAAA,KAAA,SALA;AAMA,UAAA,MAAA,EAAA,KAAA,MANA;AAOA,UAAA,SAAA,EAAA,KAAA,SAPA;AAQA,UAAA,MAAA,EAAA,KAAA;AARA,SAAA;AAUA,aAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,EAAA,WAAA,EAZA,CAcA;;AACA,aAAA,QAAA,GAAA,IAAA;AACA,aAAA,KAAA,GAAA,OAAA;AACA,aAAA,IAAA,GAAA,6CAAA;AACA,OAlBA,MAkBA;AACA,aAAA,IAAA,GAAA,wCAAA;AACA,aAAA,KAAA,GAAA,KAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA;AACA;AAzBA,GAvBA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,YAAA,EADA,CAEA;;AACA,SAAA,IAAA,KAAA,GAAA,CAAA,EAAA,KAAA,GAAA,GAAA,EAAA,KAAA,EAAA,EAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,KAAA;AACA,KALA,CAOA;;;AACA,QAAA,KAAA,YAAA,GAAA,CAAA,EAAA;AACA,WAAA,YAAA,GAAA,KAAA,YAAA;AACA,KAVA,CAYA;;;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,SAAA,KAAA,IAAA,EAAA;AACA;AACA,UAAA,aAAA,GAAA;AACA,QAAA,EAAA,EAAA,KAAA,EADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAGA,QAAA,KAAA,EAAA,KAAA,KAHA;AAIA,QAAA,gBAAA,EAAA,KAAA,gBAJA;AAKA,QAAA,SAAA,EAAA,KAAA,SALA;AAMA,QAAA,MAAA,EAAA,KAAA,MANA;AAOA,QAAA,SAAA,EAAA,KAAA;AAPA,OAAA;AASA,MAAA,YAAA,CAAA,OAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA,CAAA,aAAA,CAAA;AACA,KAZA,MAYA;AACA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,SAAA,CAAA,CAAA;AACA,WAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,WAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,WAAA,SAAA,GAAA,GAAA,CAAA,SAAA;AACA,WAAA,gBAAA,GAAA,GAAA,CAAA,gBAAA;AACA,WAAA,MAAA,GAAA,GAAA,CAAA,MAAA;AACA,WAAA,SAAA,GAAA,GAAA,CAAA,SAAA;AACA;AACA;AArFA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-row>\n      <v-col cols=\"8\">\n        <v-row class=\"justify-center mt-6\">\n          <v-img\n            class=\"\"\n            max-height=\"500px\"\n            max-width=\"500px\"\n            src=\"https://ctc-wenzel.de/wp-content/uploads/Foto-Platzhalter.jpg\"\n          ></v-img>\n          <!--3D-MODELRENDERER-->\n          <!-- <model-gltf src=\"/Users/benjamin/Desktop/uShop/Frontend/OfficeChair/scene.gltf\"></model-gltf> -->\n        </v-row>\n      </v-col>\n      <v-divider vertical></v-divider>\n      <v-col cols=\"4\">\n        <h2 class=\"text-center mt-6\">{{ Name }}</h2>\n        <br />\n        <h3 class=\"ml-3\">Kategorie: {{ Kategorie }}</h3>\n        <br />\n        <h3 class=\"ml-3\">{{ Preis }}€</h3>\n        <br />\n        <h3 class=\"ml-3 mr-3\">{{ Kurzbeschreibung }}</h3>\n        <br />\n        <v-col cols=\"4\">\n          <v-select :items=\"anzahl\" label=\"Anzahl\" dense solo v-model=\"selectAnzahl\"></v-select>\n        </v-col>\n        <v-rating\n          empty-icon=\"mdi-star-outline\"\n          full-icon=\"mdi-star\"\n          half-icon=\"$mdiStarHalfFull\"\n          length=\"5\"\n          size=\"32\"\n          :value=\"Bewertung\"\n          readonly\n          color=\"warning\"\n          background-color=\"grey\"\n        ></v-rating>\n        <br />\n        <br />\n        <v-btn outlined text class=\"lime accent-3\" @click=\"AddBasket\">\n          Zum Warenkorb hinzufügen<v-icon right>mdi-cart-outline</v-icon>\n        </v-btn>\n      </v-col>\n    </v-row>\n    <br />\n    <br />\n\n    <v-divider></v-divider>\n    <br />\n    <br />\n    <v-snackbar v-model=\"snackbar\" :timeout=\"timeout\" :color=\"color\">{{ text }}</v-snackbar>\n  </div>\n</template>\n\n<script>\n// import {ModelGltf}  from \"vue-3d-model\";\n\nexport default {\n  components: {\n    ModelGltf,\n  },\n\n  data() {\n    return {\n      ID: this.$route.params.ID,\n      Name: this.$route.params.Name,\n      Preis: this.$route.params.Preis,\n      Kurzbeschreibung: this.$route.params.Kurzbeschreibung,\n      Kategorie: this.$route.params.Kategorie,\n      Link3D: this.$route.params.Link3D,\n      Bewertung: this.$route.params.Bewertung,\n      AnzahlBasket: this.$route.params.Anzahl,\n      snackbar: false,\n      text: `Der Artikel wurde zum Warenkorb hinzugefügt`,\n      color: 'black',\n      timeout: 1000,\n      anzahl: [],\n      selectAnzahl: 0,\n    };\n  },\n  methods: {\n    AddBasket() {\n      if (this.selectAnzahl != 0) {\n        //Item dem Warenkorb hinzufügen\n        let ThisProduct = {\n          ID: this.ID,\n          Name: this.Name,\n          Preis: this.Preis,\n          Kurzbeschreibung: this.Kurzbeschreibung,\n          Kategorie: this.Kategorie,\n          Link3D: this.Link3D,\n          Bewertung: this.Bewertung,\n          Anzahl: this.selectAnzahl,\n        };\n        this.$store.dispatch('PushInWarenkorb', ThisProduct);\n\n        //Warenkorb Bestätigungsnachricht anzeigen\n        this.snackbar = true;\n        this.color = 'black';\n        this.text = 'Der Artikel wurde zum Warenkorb hinzugefügt';\n      } else {\n        this.text = 'Sie müssen eine Poduktanzahl auswählen';\n        this.color = 'red';\n        this.snackbar = true;\n      }\n    },\n  },\n  mounted() {\n    console.log(this.AnzahlBasket);\n    //Anzahl Select befüllen\n    for (let index = 1; index < 100; index++) {\n      this.anzahl.push(index);\n    }\n\n    //Anzahl aus Warenkorb bekommen\n    if (this.AnzahlBasket > 0) {\n      this.selectAnzahl = this.AnzahlBasket;\n    }\n\n    //Bei einem Seiten-Refresh wird das Letzte aufgerufene Objekt geladen\n    if (localStorage.getItem('LastObj') == null) {\n      //Objekt Speichern\n      let aktuellObjekt = {\n        ID: this.ID,\n        Name: this.Name,\n        Preis: this.Preis,\n        Kurzbeschreibung: this.Kurzbeschreibung,\n        Kategorie: this.Kategorie,\n        Link3D: this.Link3D,\n        Bewertung: this.Bewertung,\n      };\n      localStorage.setItem('LastObj', JSON.stringify(aktuellObjekt));\n    } else {\n      //Objekt Laden\n      let erg = JSON.parse(localStorage.getItem('LastObj'));\n      this.Name = erg.Name;\n      this.Preis = erg.Preis;\n      this.Kategorie = erg.Kategorie;\n      this.Kurzbeschreibung = erg.Kurzbeschreibung;\n      this.Link3D = erg.Link3D;\n      this.Bewertung = erg.Bewertung;\n    }\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/views"}]}