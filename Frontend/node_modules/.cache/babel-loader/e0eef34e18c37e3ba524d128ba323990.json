A,CAAA,MAAA,CAAA,KAAA,IAAA,EAAA;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,MAAA,CAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,uBAAA,KAAA,SAAA;AACA;AACA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AACA,KAVA;AAYA,IAAA,oBAZA,kCAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KAAA,CAAA,oBAAA,KAAA,KAAA,CAAA,aADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAGA,KAAA,CAAA,IAAA,CAAA,qCAAA,EAAA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,OADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA,QAFA;AAGA,kBAAA,KAAA,EAAA,KAAA,CAAA,KAHA;AAIA,kBAAA,QAAA,EAAA,KAAA,CAAA,SAJA;AAKA,kBAAA,OAAA,EAAA,KAAA,CAAA,OALA;AAMA,kBAAA,GAAA,EAAA,KAAA,CAAA,GANA;AAOA,kBAAA,GAAA,EAAA,KAAA,CAAA,GAPA;AAQA,kBAAA,OAAA,EAAA;AARA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,GAHA;;AAaA;AACA,gBAAA,KAAA,CAAA,WAAA,GAdA,CAgBA;;;AACA,gBAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,CAjBA,CAmBA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KAlCA;AAoCA,IAAA,SApCA,uBAoCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBAEA,MAAA,CAAA,SAAA,IAAA,MAAA,CAAA,SAFA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,gCAAA,EAAA;AACA,kBAAA,OAAA,EAAA,MAAA,CAAA,OADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAJA;;AAAA;AAAA;AAIA,gBAAA,IAJA,qBAIA,IAJA;AAUA,gBAAA,MAAA,CAAA,oBAAA,GAAA,IAAA,CAVA,CAYA;;AACA,gBAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AAnDA;AAtCA,CAAA","sourcesContent":["<template>\n  <div>\n    <!--Authenticator-->\n    <div v-if=\"showAuthenticator\" class=\"AuthenticatorDiv\">\n      <h1 style=\"text-align:center;\">Bitte gib deinen Authenticatorcode ein:</h1>\n      <v-form>\n        <v-container>\n          <v-row>\n            <v-col md=\"2\"></v-col>\n            <v-col md=\"10\"\n              ><v-text-field style=\"width: 80%\" v-model=\"authenticator\" type=\"text\"></v-text-field>\n            </v-col>\n            <v-col></v-col>\n          </v-row>\n\n          <v-row>\n            <v-col></v-col>\n            <v-col>\n              <v-btn color=\"#424242\" @click=\"authenticatorClicked\" dark>\n                Bestätigen\n              </v-btn>\n            </v-col>\n            <v-col></v-col>\n          </v-row>\n        </v-container>\n      </v-form>\n    </div>\n\n    <!--Register-->\n    <div v-bind:class=\"showAuthenticator ? 'BackgroundUnscharf' : 'BackgroundScharf'\">\n      <h1 class=\"text-center\">Register</h1>\n      <h3 class=\"text-center pt-6\">\n        Willkommen, wenn Sie noch kein Konto haben können sie hier kostelos eines erstellen.\n      </h3>\n      <br />\n      <br />\n      <v-form>\n        <v-container class=\"d-flex flex-wrap justify-space-around\">\n          <!--Vor-Nachname- Inputs-->\n          <v-row>\n            <v-col md=\"1\"></v-col>\n            <v-col md=\"10\">\n              <!--MeinSpalte wo Inputs liegen-->\n              <v-row class=\"justify-center\">\n                <v-col md=\"4\">\n                  <v-text-field\n                    label=\"Vorname\"\n                    v-model=\"Vorname\"\n                    :rules=\"rules.NormalRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n                <v-col md=\"1\"></v-col>\n                <v-col md=\"4\">\n                  <v-text-field\n                    label=\"Nachname\"\n                    v-model=\"Nachname\"\n                    :rules=\"rules.NormalRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n              </v-row>\n              <!--Email Input-->\n              <v-row class=\"justify-center\">\n                <v-col md=\"9\">\n                  <v-text-field\n                    label=\"E-Mail\"\n                    v-model=\"Email\"\n                    :rules=\"rules.EmailRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n              </v-row>\n              <!--Passwort-->\n              <v-row class=\"justify-center\">\n                <v-col md=\"4\">\n                  <v-text-field\n                    :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                    :rules=\"rules.NormalRules\"\n                    :type=\"show ? 'text' : 'password'\"\n                    label=\"Passwort\"\n                    v-model=\"Passwort1\"\n                    @click:append=\"show = !show\"\n                  />\n                </v-col>\n                <v-col md=\"1\"></v-col>\n                <v-col md=\"4\"\n                  ><v-text-field\n                    :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                    :rules=\"rules.NormalRules\"\n                    :type=\"show ? 'text' : 'password'\"\n                    label=\"Passwort\"\n                    v-model=\"Passwort2\"\n                    @click:append=\"show = !show\"\n                  />\n                </v-col>\n              </v-row>\n              <!--Adresse-->\n              <v-row class=\"justify-center\">\n                <v-col md=\"4\">\n                  <v-text-field\n                    label=\"Straße + Hausnr\"\n                    v-model=\"Strasse\"\n                    :rules=\"rules.NormalRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n                <v-col md=\"1\">\n                  <v-text-field\n                    label=\"PLZ\"\n                    v-model=\"Plz\"\n                    :rules=\"rules.NormalRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n                <v-col md=\"4\">\n                  <v-text-field\n                    label=\"Ort\"\n                    v-model=\"Ort\"\n                    :rules=\"rules.NormalRules\"\n                    hide-details=\"auto\"\n                  />\n                </v-col>\n              </v-row>\n              <!--Submit-Bttn-->\n              <v-row>\n                <v-col md=\"5\"></v-col>\n                <v-col md=\"2\">\n                  <v-btn @click=\"Submitted\">\n                    Register\n                  </v-btn>\n                </v-col>\n                <v-col md=\"5\"></v-col>\n              </v-row>\n            </v-col>\n            <v-col md=\"1\"></v-col>\n          </v-row>\n        </v-container>\n      </v-form>\n      <br />\n      <br />\n      <router-link class=\"black--text text-decoration-none\" :to=\"{ name: 'Login' }\"\n        ><p class=\"black--text text-center\">\n          Wenn sie bereits einen Account haben, können Sie sich hier anmelden: <span>Login</span>\n        </p>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      //Inputvariablen\n      authenticator: '',\n      realAutheticatorCode: '',\n      Vorname: '',\n      Nachname: '',\n      Email: '',\n      Passwort1: '',\n      Passwort2: '',\n      Strasse: '',\n      Plz: '',\n      Ort: '',\n      userArray: [],\n\n      //Variablen\n      show: false,\n      showAuthenticator: false,\n      //Anorderungen an die Inputs\n      rules: {\n        NormalRules: [(value) => !!value || 'Required.'],\n        EmailRules: [\n          (value) => !!value || 'Required.',\n          (value) => {\n            const pattern = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n            return pattern.test(value) || 'Invalid e-mail.';\n          },\n        ],\n      },\n    };\n  },\n  mounted() {\n    if (JSON.parse(localStorage.getItem('User')) != null) {\n      this.userArray = JSON.parse(localStorage.getItem('User'));\n      console.log(`User Array: ${this.userArray}`);\n    }\n  },\n  methods: {\n    ClearInputs() {\n      this.Vorname = '';\n      this.Nachname = '';\n      this.Email = '';\n      this.Strasse = '';\n      this.Passwort1 = '';\n      this.Passwort2 = '';\n      this.Plz = '';\n      this.Ort = '';\n    },\n\n    async authenticatorClicked() {\n      if (this.realAutheticatorCode === this.authenticator) {\n        // Kunden registrieren\n        const res = await axios.post('http://localhost:2410/KundeRegister', {\n          Vorname: this.Vorname,\n          Nachname: this.Nachname,\n          Email: this.Email,\n          Passwort: this.Passwort1,\n          Strasse: this.Strasse,\n          Plz: this.Plz,\n          Ort: this.Ort,\n          IsAdmin: false,\n        });\n        //Leert alle Inputs\n        this.ClearInputs();\n\n        //Authenticator-Mode off\n        this.showAuthenticator = false;\n\n        //Wenn fertig zur Login-Seite weiterleiten\n        this.$router.push('Login');\n      }\n    },\n\n    async Submitted() {\n      //Beide Passwörter vergleichen\n      if (this.Passwort1 == this.Passwort2) {\n        //Auth-Code bekommen\n        const { data: code } = await axios.post('http://localhost:2410/SendCode', {\n          Vorname: this.Vorname,\n          Nachname: this.Nachname,\n          Email: this.Email,\n        });\n\n        this.realAutheticatorCode = code;\n\n        //Authentificator-Mode on\n        this.showAuthenticator = true;\n      }\n    },\n  },\n};\n</script>\n\n<style>\nspan {\n  color: rgb(9, 156, 9);\n  font-weight: bold;\n}\n\n.AuthenticatorDiv {\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n  z-index: 2;\n  width: 30%;\n  height: 15%;\n  background: white;\n  border-radius: 45px;\n  padding-top: 15%;\n}\n\n.BackgroundUnscharf {\n  filter: blur(10px);\n}\n.BackgroundScharf {\n  filter: none;\n}\n</style>\n"],"sourceRoot":"src/views"}]}