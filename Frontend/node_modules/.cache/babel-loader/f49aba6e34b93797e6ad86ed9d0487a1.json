CA,WAAA,SAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,MAAA,CAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,uBAAA,KAAA,SAAA;AACA;AACA,GAlCA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AACA,KAVA;AAYA,IAAA,SAZA,uBAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBAEA,KAAA,CAAA,SAAA,IAAA,KAAA,CAAA,SAFA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,qCAAA,EAAA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,OADA;AAEA,kBAAA,QAAA,EAAA,KAAA,CAAA,QAFA;AAGA,kBAAA,KAAA,EAAA,KAAA,CAAA,KAHA;AAIA,kBAAA,QAAA,EAAA,KAAA,CAAA,SAJA;AAKA,kBAAA,OAAA,EAAA,KAAA,CAAA,OALA;AAMA,kBAAA,GAAA,EAAA,KAAA,CAAA,GANA;AAOA,kBAAA,GAAA,EAAA,KAAA,CAAA,GAPA;AAQA,kBAAA,OAAA,EAAA;AARA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAcA;AACA,gBAAA,KAAA,CAAA,WAAA,GAfA,CAgBA;;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA;AA/BA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h1 class=\"text-center\">Register</h1>\r\n    <h3 class=\"text-center pt-6\">\r\n      Willkommen, wenn Sie noch kein Konto haben können sie hier kostelos eines erstellen.\r\n    </h3>\r\n    <br />\r\n    <br />\r\n    <v-form>\r\n      <v-container class=\"d-flex flex-wrap justify-space-around\">\r\n        <!--Vor-Nachname- Inputs-->\r\n        <v-row>\r\n          <v-col md=\"1\"></v-col>\r\n          <v-col md=\"10\">\r\n            <!--MeinSpalte wo Inputs liegen-->\r\n            <v-row class=\"justify-center\">\r\n              <v-col md=\"4\">\r\n                <v-text-field\r\n                  label=\"Vorname\"\r\n                  v-model=\"Vorname\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n              <v-col md=\"1\"></v-col>\r\n              <v-col md=\"4\">\r\n                <v-text-field\r\n                  label=\"Nachname\"\r\n                  v-model=\"Nachname\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n            </v-row>\r\n            <!--Email Input-->\r\n            <v-row class=\"justify-center\">\r\n              <v-col md=\"9\">\r\n                <v-text-field\r\n                  label=\"E-Mail\"\r\n                  v-model=\"Email\"\r\n                  :rules=\"rules.EmailRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n            </v-row>\r\n            <!--Passwort-->\r\n            <v-row class=\"justify-center\">\r\n              <v-col md=\"4\">\r\n                <v-text-field\r\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  :type=\"show ? 'text' : 'password'\"\r\n                  name=\"input-10-1\"\r\n                  label=\"Passwort\"\r\n                  v-model=\"Passwort1\"\r\n                  @click:append=\"show = !show\"\r\n                />\r\n              </v-col>\r\n              <v-col md=\"1\"></v-col>\r\n              <v-col md=\"4\"\r\n                ><v-text-field\r\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  :type=\"show ? 'text' : 'password'\"\r\n                  name=\"input-10-1\"\r\n                  label=\"Passwort\"\r\n                  v-model=\"Passwort2\"\r\n                  @click:append=\"show = !show\"\r\n                />\r\n              </v-col>\r\n            </v-row>\r\n            <!--Adresse-->\r\n            <v-row class=\"justify-center\">\r\n              <v-col md=\"4\">\r\n                <v-text-field\r\n                  label=\"Straße + Hausnr\"\r\n                  v-model=\"Strasse\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n              <v-col md=\"1\">\r\n                <v-text-field\r\n                  label=\"PLZ\"\r\n                  v-model=\"Plz\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n              <v-col md=\"4\">\r\n                <v-text-field\r\n                  label=\"Ort\"\r\n                  v-model=\"Ort\"\r\n                  :rules=\"rules.NormalRules\"\r\n                  hide-details=\"auto\"\r\n                />\r\n              </v-col>\r\n            </v-row>\r\n            <!--Submit-Bttn-->\r\n            <v-row>\r\n              <v-col md=\"5\"></v-col>\r\n              <v-col md=\"2\">\r\n                <v-btn @click=\"Submitted\">\r\n                  Register\r\n                </v-btn>\r\n              </v-col>\r\n              <v-col md=\"5\"></v-col>\r\n            </v-row>\r\n          </v-col>\r\n          <v-col md=\"1\"></v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-form>\r\n    <br />\r\n    <br />\r\n    <router-link class=\"black--text text-decoration-none\" :to=\"{ name: 'Login' }\"\r\n      ><p class=\"black--text text-center\">\r\n        Wenn sie bereits einen Account haben, können Sie sich hier anmelden: <span>Login</span>\r\n      </p>\r\n    </router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      //Inputvariablen\r\n      Vorname: '',\r\n      Nachname: '',\r\n      Email: '',\r\n      Passwort1: '',\r\n      Passwort2: '',\r\n      Strasse: '',\r\n      Plz: '',\r\n      Ort: '',\r\n      userArray: [],\r\n\r\n      //Variablen\r\n      show: false,\r\n      //Anorderungen an die Inputs\r\n      rules: {\r\n        NormalRules: [(value) => !!value || 'Required.'],\r\n        EmailRules: [\r\n          (value) => !!value || 'Required.',\r\n          (value) => {\r\n            const pattern = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n            return pattern.test(value) || 'Invalid e-mail.';\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    if (JSON.parse(localStorage.getItem('User')) != null) {\r\n      this.userArray = JSON.parse(localStorage.getItem('User'));\r\n      console.log(`User Array: ${this.userArray}`);\r\n    }\r\n  },\r\n  methods: {\r\n    ClearInputs() {\r\n      this.Vorname = '';\r\n      this.Nachname = '';\r\n      this.Email = '';\r\n      this.Strasse = '';\r\n      this.Passwort1 = '';\r\n      this.Passwort2 = '';\r\n      this.Plz = '';\r\n      this.Ort = '';\r\n    },\r\n\r\n    async Submitted() {\r\n      //Beide Passwörter vergleichen\r\n      if (this.Passwort1 == this.Passwort2) {\r\n        //Kunden registrieren\r\n        const res = await axios.post('http://localhost:2410/KundeRegister', {\r\n          Vorname: this.Vorname,\r\n          Nachname: this.Nachname,\r\n          Email: this.Email,\r\n          Passwort: this.Passwort1,\r\n          Strasse: this.Strasse,\r\n          Plz: this.Plz,\r\n          Ort: this.Ort,\r\n          IsAdmin: false,\r\n        });\r\n        //Leert alle Inputs\r\n        this.ClearInputs();\r\n        //Wenn fertig zur Login-Seite weiterleiten\r\n        this.$router.push('Login');\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\nspan {\r\n  color: rgb(9, 156, 9);\r\n  font-weight: bold;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}