\n            <!--MeinSpalte wo Inputs liegen-->\n            <v-row class=\"justify-center\">\n              <v-col md=\"4\">\n                <v-text-field\n                  label=\"Vorname\"\n                  v-model=\"Vorname\"\n                  :rules=\"rules.NormalRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n              <v-col md=\"1\"></v-col>\n              <v-col md=\"4\">\n                <v-text-field\n                  label=\"Nachname\"\n                  v-model=\"Nachname\"\n                  :rules=\"rules.NormalRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n            </v-row>\n            <!--Email Input-->\n            <v-row class=\"justify-center\">\n              <v-col md=\"9\">\n                <v-text-field\n                  label=\"E-Mail\"\n                  v-model=\"Email\"\n                  :rules=\"rules.EmailRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n            </v-row>\n            <!--Passwort-->\n            <v-row class=\"justify-center\">\n              <v-col md=\"4\">\n                <v-text-field\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                  :rules=\"rules.NormalRules\"\n                  :type=\"show ? 'text' : 'password'\"\n                  name=\"input-10-1\"\n                  label=\"Passwort\"\n                  v-model=\"Passwort1\"\n                  @click:append=\"show = !show\"\n                />\n              </v-col>\n              <v-col md=\"1\"></v-col>\n              <v-col md=\"4\"\n                ><v-text-field\n                  :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                  :rules=\"rules.NormalRules\"\n                  :type=\"show ? 'text' : 'password'\"\n                  name=\"input-10-1\"\n                  label=\"Passwort\"\n                  v-model=\"Passwort2\"\n                  @click:append=\"show = !show\"\n                />\n              </v-col>\n            </v-row>\n            <!--Adresse-->\n            <v-row class=\"justify-center\">\n              <v-col md=\"4\">\n                <v-text-field\n                  label=\"Straße + Hausnr\"\n                  :rules=\"rules.NormalRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n              <v-col md=\"1\">\n                <v-text-field\n                  label=\"PLZ\"\n                  v-model=\"Plz\"\n                  :rules=\"rules.NormalRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n              <v-col md=\"4\">\n                <v-text-field\n                  label=\"Ort\"\n                  v-model=\"Ort\"\n                  :rules=\"rules.NormalRules\"\n                  hide-details=\"auto\"\n                />\n              </v-col>\n            </v-row>\n            <!--Submit-Bttn-->\n            <v-row>\n              <v-col md=\"5\"></v-col>\n              <v-col md=\"2\">\n                <v-btn @click=\"TestSubmit\">\n                  Register\n                </v-btn>\n              </v-col>\n              <v-col md=\"5\"></v-col>\n            </v-row>\n\n\n          </v-col>\n          <v-col md=\"1\"></v-col>\n        </v-row>\n      </v-container>\n    </v-form>\n    <br />\n    <br />\n    <router-link class=\"black--text text-decoration-none\" :to=\"{ name: 'Login' }\"\n      ><p class=\"black--text text-center\">\n        Wenn sie bereits einen Account haben, können Sie sich hier anmelden: <span>Login</span>\n      </p>\n    </router-link>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      //Inputvariablen\n      Vorname: '',\n      Nachname: '',\n      Email: '',\n      Passwort1: '',\n      Passwort2: '',\n      Strasse: '',\n      Plz: '',\n      Ort: '',\n\n      //Variablen\n      show: false,\n      //Anorderungen an die Inputs\n      rules: {\n        NormalRules: [(value) => !!value || 'Required.'],\n        EmailRules: [\n          (value) => !!value || 'Required.',\n          (value) => {\n            const pattern = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n            return pattern.test(value) || 'Invalid e-mail.';\n          },\n        ],\n      },\n    };\n  },\n\n  methods: {\n    TestSubmit() {\n      //Beide Passwörter vergleichen\n      if(Passwort1 == Passwort2){\n        console.log(\"Submitted\")\n      }\n    },\n  },\n};\n</script>\n\n<style>\nspan {\n  color: rgb(9, 156, 9);\n  font-weight: bold;\n}\n</style>\n"]}]}