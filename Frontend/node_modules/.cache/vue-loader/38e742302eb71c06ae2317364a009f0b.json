{"remainingRequest":"/Users/benjamin/Desktop/uShop/Frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/benjamin/Desktop/uShop/Frontend/src/views/Product_Detail.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/benjamin/Desktop/uShop/Frontend/src/views/Product_Detail.vue","mtime":1633003188346},{"path":"/Users/benjamin/Desktop/uShop/Frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/benjamin/Desktop/uShop/Frontend/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/Users/benjamin/Desktop/uShop/Frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/benjamin/Desktop/uShop/Frontend/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Product_Detail.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwDA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Product_Detail.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <v-row>\n      <v-col cols=\"8\">\n        <v-row class=\"justify-center mt-6\">\n          <v-img\n            class=\"\"\n            max-height=\"500px\"\n            max-width=\"500px\"\n            src=\"https://ctc-wenzel.de/wp-content/uploads/Foto-Platzhalter.jpg\"\n          ></v-img>\n        </v-row>\n      </v-col>\n      <v-divider vertical></v-divider>\n      <v-col cols=\"4\">\n        <h2 class=\"text-center mt-6\">{{ Name }}</h2>\n        <br />\n        <h3 class=\"ml-3\">Kategorie: {{ Kategorie }}</h3>\n        <br />\n        <h3 class=\"ml-3\">{{ Preis }}€</h3>\n        <br />\n        <h3 class=\"ml-3 mr-3\">{{ Kurzbeschreibung }}</h3>\n        <br />\n        <v-rating\n          empty-icon=\"mdi-star-outline\"\n          full-icon=\"mdi-star\"\n          half-icon=\"$mdiStarHalfFull\"\n          length=\"5\"\n          size=\"32\"\n          :value=\"Bewertung\"\n          readonly\n          color=\"warning\"\n          background-color=\"grey\"\n        ></v-rating>\n        <br />\n        <br />\n\n        <v-btn outlined text class=\"lime accent-3\" @click=\"AddBasket\">\n          Zum Warenkorb hinzufügen<v-icon right>mdi-cart-outline</v-icon>\n        </v-btn>\n      </v-col>\n    </v-row>\n    <br />\n    <br />\n\n    <v-divider></v-divider>\n    <router-link :to=\"{ name: 'Shop' }\" class=\"text-decoration-none\">\n      <v-btn outlined text class=\"lime accent-3\">Zurück zu den Produkten</v-btn>\n    </router-link>\n    <br />\n    <br />\n    <v-snackbar v-model=\"snackbar\" :timeout=\"timeout\">{{ text }}</v-snackbar>\n  </div>\n</template>\n\n<script>\nimport eventBus from '../eventbus';\n\nexport default {\n  data() {\n    return {\n      ID: this.$route.params.ID,\n      Name: this.$route.params.Name,\n      Preis: this.$route.params.Preis,\n      Kurzbeschreibung: this.$route.params.Kurzbeschreibung,\n      Kategorie: this.$route.params.Kategorie,\n      Link3D: this.$route.params.Link3D,\n      Bewertung: this.$route.params.Bewertung,\n      snackbar: false,\n      text: `Der Artikel wurde zum Warenkorb hinzugefügt`,\n      timeout: 1000,\n    };\n  },\n  methods: {\n    AddBasket() {\n      //Item dem Warenkorb hinzufügen\n      let ThisProduct = {\n        ID: this.ID,\n        Name: this.Name,\n        Preis: this.Preis,\n        Kurzbeschreibung: this.Kurzbeschreibung,\n        Kategorie: this.Kategorie,\n        Link3D: this.Link3D,\n        Bewertung: this.Bewertung,\n      };\n      this.$store.dispatch('PushInWarenkorb', ThisProduct);\n\n      //Warenkorb Bestätigungsnachricht anzeigen\n      this.snackbar = true;\n    },\n  },\n  mounted() {\n    //Bei einem Seiten-Refresh wird das Letzte aufgerufene Objekt geladen\n    if (localStorage.getItem('LastObj') == null) {\n      //Objekt Speichern\n      let aktuellObjekt = {\n        ID: this.ID,\n        Name: this.Name,\n        Preis: this.Preis,\n        Kurzbeschreibung: this.Kurzbeschreibung,\n        Kategorie: this.Kategorie,\n        Link3D: this.Link3D,\n        Bewertung: this.Bewertung,\n      };\n      localStorage.setItem('LastObj', JSON.stringify(aktuellObjekt));\n    } else {\n      //Objekt Laden\n      let erg = JSON.parse(localStorage.getItem('LastObj'));\n      this.Name = erg.Name;\n      this.Preis = erg.Preis;\n      this.Kategorie = erg.Kategorie;\n      this.Kurzbeschreibung = erg.Kurzbeschreibung;\n      this.Link3D = erg.Link3D;\n      this.Bewertung = erg.Bewertung;\n    }\n  },\n};\n</script>\n\n<style></style>\n"]}]}